function handleOnLoad() {
   
var newheight;
    var newwidth;

    if(document.getElementById){
        newheight = document.getElementById("myframe").contentWindow.document .body.scrollHeight;
        newwidth = document.getElementById("myframe").contentWindow.document .body.scrollWidth;
    }

    document.getElementById("myframe").height = (newheight) + "px";
    document.getElementById("myframe").width = (newwidth) + "px";

 var iframe = document.getElementById("myframe");
    iframe.setAttribute("src","https://twitter.com/");
    
}


